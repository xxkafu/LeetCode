// solution:
// easy
class Solution {
public:
    uint32_t reverseBits(uint32_t n) {
        uint32_t result = 0;
        uint32_t mask = 1;
        for(int i =1 ; i<33; i++) {
            result = (result<<1) | (n&mask);
            n >>=1;
        }
        return result;
        
    }
};

